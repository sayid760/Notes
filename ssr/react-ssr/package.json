{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "rimraf dist && concurrently \"npm run ssr\" \" npm run csr \"",
    "ssr": "cross-env NODE_ENV=development webpack --config ./build/webpack.server.config.js --watch",
    "csr": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.client.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js --watch",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js --watch",
    "build": "rimraf dist && npm run build:client &&  npm run build:server ",
    "start:node": "nodemon ./dist/server/app.js",
    "b": "rimraf dist && npm run build:client",
    "dev": "cross-env NODE_ENV=development nodemon ./src/index-server.js --watch"
  },
